{
  "name": "juankno/laravel-repository",
  "description": "This package simplifies working with the Repository Pattern in Laravel by automatically generating repository files, contracts, and bindings.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Juankno",
      "email": "tu-email@example.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Juankno\\Repository\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Juankno\\Repository\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Juankno\\Repository\\Providers\\RepositoryServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}